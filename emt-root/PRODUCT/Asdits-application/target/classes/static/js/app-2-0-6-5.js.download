          {
              $scope.imgFile = '';
              var files = file.files[0];
              if ( /\.(jpe?g|png|gif)$/i.test(files.name) ) {
                var reader = new FileReader();
                reader.addEventListener("load", function () {
                  var obj = {};
                  obj.name = file.name;
                  obj.size = file.size;
                  obj.type = file.type;
                  obj.base64 = this.result;
                  $scope.$apply(function () {
                      $scope.imgFile = obj;
                  });
                }, false);
               reader.readAsDataURL(files);

              }
          }

