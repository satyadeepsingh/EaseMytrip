                        if($scope.status != 21){
                          $(".ofd.active").addClass("gbg");
                        } else {
                          $(".ofd.active").addClass("red");                          
                        }
                        $('.strip3.active0').addClass('gbg');
                        $(".delivered.active").addClass("gbg");
                    }, 2400);
                }
            }
        }
    }
