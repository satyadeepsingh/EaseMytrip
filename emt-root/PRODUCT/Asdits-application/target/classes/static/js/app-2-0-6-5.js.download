                }

                var data =
                {
                    quantity: quantity,
                    action: action
                }

                AppService.put('inventory/' + currentId +"/update", true, data)
                .success(function (data)
                {
                    $scope.isPreloader = false;
                    $state.go($state.current, {}, {reload: true});
