            function openLabelModal(order, isCourierCompany, $event) {
              if((1 + $localStorage.USER.shipment_count) > 5 && [0,3].indexOf($localStorage.USER.kyc_status) != -1 && $localStorage.USER.kyc_shipment_check){
                loaderFactory.hide();
