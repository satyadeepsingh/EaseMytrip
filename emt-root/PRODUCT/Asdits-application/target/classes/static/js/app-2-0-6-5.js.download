                  case 'app.companyBankDetails':
                    if(data.onboarding_completed_now){
                      var modalInstance = $uibModal.open({
                        animation: true,
                        templateUrl: 'app/views/modals/preview-company-details.html',
                        controller:'PreviewCompanyDetailsController',
                        size: 'lg',
