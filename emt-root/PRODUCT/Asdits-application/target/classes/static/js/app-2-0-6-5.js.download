                        $scope.pickupServiceableStatus ='fa-times-circle text-danger';
                        }
                        else{}

                        if(pincode_for == 'delivery')
                        {
                            $scope.DeliveryServiceableStatus ='fa-times-circle text-danger';
                        }
                        else{}
                    }
                    else
                    {
                        AppService.isAuth(error);
                    }
                })
            }

