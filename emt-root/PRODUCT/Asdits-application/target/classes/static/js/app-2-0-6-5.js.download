                width: $('.totalqty').outerWidth(),
                tableClass: 'inventoryList'
            }
            var orderdate = {
                className : 'orderdate',
                width: $('.orderdate').outerWidth(),
            }
            

            //Quantity Edit popover//
            $scope.showQuentyPopup = function($event, listId)
