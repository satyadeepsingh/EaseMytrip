            $scope.filterModels = {
                courier_id: angular.isDefined(currentFilters.courier_id) ? currentFilters.courier_id : "",
                shipment_status: angular.isDefined(currentFilters.shipment_status) ? currentFilters.shipment_status : ""
            }

