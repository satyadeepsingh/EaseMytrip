                    if (data.status == '404') {
                      Notify.alert(data.message, {status: 'danger', timeout: 7000 });
                      loaderFactory.hide();
                      return false;
                    } else {
                      var item = {
                        "order": order,
                        "data": data.data,
                        "is_return": 1
                      }
                      var modalInstance = $uibModal.open({
                        animation: true,
                        templateUrl: 'orderlabel.html',
